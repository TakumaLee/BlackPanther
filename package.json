{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:qa": "cp .env.qa .env.local && next dev --turbopack",
    "dev:prod": "cp .env.production .env.local && next dev --turbopack",
    "build": "next build --turbopack",
    "build:dev": "cp .env.development .env.local && next build --turbopack",
    "build:qa": "cp .env.qa .env.local && next build --turbopack",
    "build:prod": "cp .env.production .env.local && next build --turbopack",
    "start": "next start",
    "start:dev": "cp .env.development .env.local && next start",
    "start:qa": "cp .env.qa .env.local && next start",
    "start:prod": "cp .env.production .env.local && next start",
    "lint": "eslint",
    "clean": "rm -rf .next node_modules/.cache",
    "env:dev": "cp .env.development .env.local && echo 'Switched to development environment'",
    "env:qa": "cp .env.qa .env.local && echo 'Switched to QA environment'",
    "env:prod": "cp .env.production .env.local && echo 'Switched to production environment'",
    "env:check": "node -e \"console.log('Current environment:', process.env.NEXT_PUBLIC_APP_ENV || 'Not set')\""
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.89.0",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "puppeteer": "^24.19.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/react-query-devtools": "^5.89.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
